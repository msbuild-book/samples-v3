<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<connectionStrings xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  
  <add name="RecordsDb" connectionString="db.contoso.com;Initial Catalog=RecordsDb-RELEASE;Integrated Security=true"
         xdt:Transform="Replace" xdt:Locator="Match(name)"/>
  <add name="InventoryDb" connectionString="db.constos.com;Initial Catalog=InventoryDb-RELEASE;Integrated Security=true"
       xdt:Transform="Insert" xdt:Locator="Match(name)" />
  
</connectionStrings>